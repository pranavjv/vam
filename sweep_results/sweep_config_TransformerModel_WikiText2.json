{
  "method": "bayes",
  "metric": {
    "name": "optimization_metric",
    "goal": "minimize"
  },
  "parameters": {
    "model": {
      "value": "TransformerModel"
    },
    "dataset": {
      "value": "WikiText2"
    },
    "dataset_size": {
      "value": "small"
    },
    "epochs": {
      "value": 3
    },
    "eta": {
      "distribution": "uniform",
      "min": 0.0001,
      "max": 0.01
    },
    "beta1": {
      "distribution": "uniform",
      "min": 0.8,
      "max": 0.99
    },
    "beta2": {
      "distribution": "uniform",
      "min": 0.99,
      "max": 0.9999
    },
    "beta3": {
      "distribution": "uniform",
      "min": 0.1,
      "max": 2.0
    },
    "power": {
