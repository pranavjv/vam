{
  "method": "bayes",
  "metric": {
    "name": "optimization_metric",
    "goal": "minimize"
  },
  "parameters": {
    "optimizer_name": {
      "value": "ADAM"
    },
    "dataset_size": {
      "value": "full"
    },
    "epochs": {
      "value": 100
    },
    "batch_size": {
      "value": 64
    },
    "gamma": {
      "value": 0.99
    },
    "entropy_coef": {
      "distribution": "uniform",
      "min": 0.001,
      "max": 0.05
    },
    "hidden_dim": {
      "value": 128
    },
    "adam_lr": {
      "distribution": "log_uniform_values",
      "min": 1e-05,
      "max": 0.1
    },
    "adam_beta1": {
      "value": 0.9
    },
    "adam_beta2": {
      "value": 0.999
    },
    "adam_weight_decay": {
      "value": 1e-05
    },
    "adam_eps": {
      "value": 1e-08
    }
  }
}