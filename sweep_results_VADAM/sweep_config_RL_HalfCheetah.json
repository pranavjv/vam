{
  "method": "bayes",
  "metric": {
    "name": "optimization_metric",
    "goal": "minimize"
  },
  "parameters": {
    "optimizer_name": {
      "value": "VADAM"
    },
    "dataset_size": {
      "value": "full"
    },
    "epochs": {
      "value": 100
    },
    "batch_size": {
      "value": 64
    },
    "gamma": {
      "value": 0.99
    },
    "entropy_coef": {
      "distribution": "uniform",
      "min": 0.001,
      "max": 0.05
    },
    "hidden_dim": {
      "value": 128
    },
    "eta": {
      "distribution": "log_uniform_values",
      "min": 1e-05,
      "max": 0.1
    },
    "beta1": {
      "value": 0.9
    },
    "beta2": {
      "value": 0.999
    },
    "beta3": {
      "distribution": "uniform",
      "min": 0.1,
      "max": 2.0
    },
    "power": {
      "value": 2
    },
    "normgrad": {
      "values": [
        true,
        false
      ]
    },
    "lr_cutoff": {
      "distribution": "int_uniform",
      "min": 5,
      "max": 30
    },
    "weight_decay": {
      "value": 1e-05
    },
    "eps": {
      "value": 1e-08
    }
  }
}